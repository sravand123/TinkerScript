try {
   2 ** nil;
} catch (error) {
    println(error.message); // expect: Operands must be numbers.
}

try {
    nil ** 2;
} catch (error) {
    println(error.message); // expect: Operands must be numbers.
}

try {
   2 ** "2";
} catch (error) {
    println(error.message); // expect: Operands must be numbers.
}

