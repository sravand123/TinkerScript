str := "Testing";

println(str[0:1]); // expect: T
println(str[0:2]); // expect: Te
println(str[1:3]); // expect: es
println(str[2:4]); // expect: st
println(str[0:5]); // expect: Testi
println(str[0:6]); // expect: Testin

println(str[:1]); // expect: T
