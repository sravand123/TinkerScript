// Test case for variadic function in Lox

fun sum(...numbers) {
    total := 0;
    for (i:= 0; i < len(numbers); i+=1) {
        total += numbers[i];
    }
    return total;
}

fun testSum() {
    var result = sum(1, 2, 3, 4, 5);
    if (result == 15) {
        print("Test passed!");
    } else {
        print("Test failed!");
    }
}

testSum();