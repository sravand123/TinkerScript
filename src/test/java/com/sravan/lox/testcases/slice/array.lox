a := [1,2,3,4];

println(a[1:2]); // expect: [2]
println(a[1:3]); // expect: [2, 3]
println(a[1:4]); // expect: [2, 3, 4]
println(a[1:5]); // expect: [2, 3, 4]
println(a[1:6]); // expect: [2, 3, 4]

println(a[:2]);  // expect: [1, 2]
println(a[:3]);  // expect: [1, 2, 3]
println(a[:4]);  // expect: [1, 2, 3, 4]
println(a[:]);   // expect: [1, 2, 3, 4]
println(a[:5]);  // expect: [1, 2, 3, 4]
println(a[:6]);  // expect: [1, 2, 3, 4]

println(a[1:1000]); // expect: [2, 3, 4]
println(a[:1000]); // expect: [1, 2, 3, 4]